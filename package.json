{
  "name": "admin-template",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "nest": "nest",
    "next": "cd src/client && next",
    "prebuild": "rimraf dist && rimraf ./src/client/.next && rimraf ./src/client/styles/styles.css",
    "build": "cross-env NODE_ENV=production nest build && yarn run tailwind:prodbuild && cd src/client && next build",
    "tailwind:prodbuild": "cross-env NODE_ENV=production yarn run tailwind:build",
    "tailwind:build": "tailwind build -i ./src/client/styles/tailwind.css -o ./src/client/styles/styles.css",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "cross-env NODE_ENV=production node dist/main.js",
    "prestart:dev": "yarn run tailwind:build",
    "start:dev": "nest start --watch --preserveWatchOutput",
    "start:dev-hmr": "nest build --webpack --webpackPath webpack-hmr.config.js --watch",
    "start:debug": "nest start --debug --watch --preserveWatchOutput",
    "start:prod": "NODE_ENV=production node dist/src/server/main",
    "start:doc": "npx @compodoc/compodoc -p tsconfig.json -s",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage --passWithNoTests",
    "test:badges": "yarn run test:cov && jest-coverage-badges --output ./badges",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "ci": "yarn install --frozen-lockfile",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@nestjs/common": "^7.6.15",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^7.6.15",
    "@nestjs/graphql": "^7.11.0",
    "@nestjs/platform-express": "^7.6.15",
    "@nestjs/typeorm": "^7.1.5",
    "apollo-server-express": "^2.25.1",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "compression": "^1.7.4",
    "csurf": "^1.11.0",
    "express-mysql-session": "^2.1.6",
    "express-session": "^1.17.2",
    "graphql": "^15.5.0",
    "graphql-tools": "^7.0.5",
    "helmet": "^4.6.0",
    "joi": "^17.4.0",
    "lodash": "^4.17.21",
    "mysql2": "^2.2.5",
    "next": "^10.2.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.6",
    "typeorm": "^0.2.34"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.11",
    "@nestjs/cli": "^7.6.0",
    "@nestjs/schematics": "^7.3.0",
    "@nestjs/testing": "^7.6.15",
    "@types/express": "^4.17.11",
    "@types/express-session": "^1.17.3",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^26.0.22",
    "@types/lodash": "^4.14.170",
    "@types/node": "^14.14.36",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.7",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "autoprefixer": "^10.2.6",
    "cross-env": "^7.0.3",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "jest-coverage-badges": "^1.1.2",
    "postcss": "^8.3.4",
    "prettier": "^2.2.1",
    "run-script-webpack-plugin": "^0.0.11",
    "supertest": "^6.1.3",
    "tailwindcss": "^2.1.4",
    "ts-jest": "^26.5.4",
    "ts-loader": "^8.0.18",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.3",
    "webpack": "^5.39.1",
    "webpack-node-externals": "^3.0.0"
  },
  "jest": {
    "coverageReporters": [
      "json-summary",
      "text",
      "lcov"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
